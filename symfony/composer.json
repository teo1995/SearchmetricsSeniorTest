{
    "name": "searchmetrics/php-senior-test",
    "description": "Contains the sources for the Senior Backend Developer position tests.",
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "Searchmetrics GmbH",
            "homepage": "https://www.searchmetrics.com/"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-gmp": "*",
        "ext-iconv": "*",
        "ext-mysqli": "*",
        "friendsofsymfony/rest-bundle": "^2.5",
        "phpunit/phpunit": "^7",
        "sensio/framework-extra-bundle": "^5.2",
        "symfony/console": "4.2.*",
        "symfony/dotenv": "4.2.*",
        "symfony/flex": "^1.1",
        "symfony/form": "4.2.*",
        "symfony/framework-bundle": "4.2.*",
        "symfony/orm-pack": "^1.0",
        "symfony/serializer": "4.2.*",
        "symfony/twig-bundle": "4.2.*",
        "symfony/yaml": "4.2.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },

    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.2.*"
        }
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.11",
        "symfony/phpunit-bridge": "*"
    }
}
